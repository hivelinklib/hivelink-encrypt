{
  "name": "hivelink/encrypt",
  "description": "Auto Encrypt and Decrypt Database through Eloquent With HivelinkTeam",
  "type": "library",
  "license": "MIT",
  "minimum-stability": "stable",
  "authors": [
    {
      "name": "Telka",
      "email": "telkaweb@gmail.com"
    }
  ],
  "require": {},
  "autoload": {
    "psr-4": {
      "Hivelink\\DBEncryption\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
        "Hivelink\\DBEncryption\\Tests\\": "tests",
        "Hivelink\\DBEncryption\\Tests\\Database\\Factories\\": "tests/database/factories"
    }
  },
  "require-dev": {
    "orchestra/testbench": "^6.0",
    "phpunit/phpunit": "^9.4"
  },
  "extra": {
    "laravel": {
        "providers": [
            "Hivelink\\DBEncryption\\Providers\\DBEncryptionServiceProvider"
        ]
    }
  },
  "scripts": {
    "test": "vendor/bin/phpunit",
    "test-f": "vendor/bin/phpunit --filter"
  }
}
